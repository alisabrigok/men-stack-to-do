{
  "name": "men-stack-to-do",
  "version": "0.1.0",
  "description": "A RESTful to-do application with authentication developed using MEN stack and Sass",
  "main": "app.js",
  "scripts": {
    "build-css": "node-sass public/assets/sass/main.scss public/assets/css/style.css",
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "dev": "concurrently \"node app.js\" \"npm run watch-css\"",
    "start": "set NODE_ENV=production node ./app"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "connect-flash": "^0.1.1",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "method-override": "^2.3.10",
    "mongoose": "^4.13.14",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^4.5.0"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "node-sass": "^4.9.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alisabrigok/men-stack-to-do.git"
  },
  "keywords": [
    "men",
    "stack",
    "restful",
    "to-do",
    "application",
    "rest",
    "todo",
    "mvc",
